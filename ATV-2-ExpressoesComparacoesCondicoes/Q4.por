programa {
  funcao inicio() {
    
    inteiro temperatura

    escreva("Me de a temperatura atual ")
    leia(temperatura)

    se(temperatura>0){
      escreva("O valor informado é positivo.")
    }

    se(temperatura<0){
      escreva("O valor informado é negativo.")
    }

    se(temperatura==0){
      escreva("O valor informado é nulo.")
    }
  }
}
